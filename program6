#include<stdio.h>
int main()
{
    int a,b,sum,difference,product,division;
    printf("enter two numbers");
    scanf("%d,%d",&a,&b);
    
    sum=a+b;
    difference=a-b;
    product=a*b;
    division=a/b;
printf("sum:%d\n",sum);
printf("difference:%d\n",difference);
printf("product:%d\n",product);
printf("division:%d\n",division);
return 0;

}
